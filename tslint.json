{
  "extends": ["tslint-config-airbnb"],
  "rules": {
    "import-name": [true, { "styledComponents": "styled" }],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "ter-arrow-parens": [true, "as-needed"]
  }
}
